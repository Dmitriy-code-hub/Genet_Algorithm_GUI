<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>





<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.genet_algoritm_gui.Controller">
    <Label text="Enter number of places">
    </Label>
    <TextField fx:id="tf_numberplaces" text="10">
        </TextField>
    <Label text="Enter start population">
    </Label>
    <TextField fx:id="tf_initialisepopulation" text="100">
    </TextField>

    <Label text="Enter number of iterations">
    </Label>
    <TextField fx:id="tf_numberiterations" text="100">

    </TextField>

    <Label text="Enter % of mutation">
    </Label>
    <TextField fx:id="tf_mutationRate" text="0.05">
    </TextField>



    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Label fx:id="welcomeText"/>
    <Button text="Build Diagram" onAction="#onHelloButtonClick"/>
</VBox>
